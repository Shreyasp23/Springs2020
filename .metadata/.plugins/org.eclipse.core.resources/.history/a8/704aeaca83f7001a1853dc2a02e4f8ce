package com.code.springdemo;

import java.util.ArrayList;
import java.util.List;
import java.util.Random;

public class HappyFornuteService implements FortuneService {

	@Override
	public String getFortune() {
		List<String> fortuneList= new ArrayList<String>();
		fortuneList.add("good luck");
		fortuneList.add("good day");
		fortuneList.add("good night");
		
		Random ran = new Random(3);
		
		String result=fortuneList.get(ran.nextInt(2));
		return result;
	}

	@Override
	public String getSpecialfortune() {
		
		return "You got very lucky!!!";
	}

	
}
